    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <org.chromium.chrome.browser.toolbar.circlemenu.widget.ArcLayout
        android:id="@+id/arcmenu_item_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

    </org.chromium.chrome.browser.toolbar.circlemenu.widget.ArcLayout>


    <FrameLayout
        android:id="@+id/layArcMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <org.chromium.chrome.browser.toolbar.circlemenu.widget.FloatingActionButton
            android:id="@+id/fabArcMenu"
            android:layout_width="wrap_content"
            android:background="@drawable/ic_center_action_button"
            android:layout_height="wrap_content"
            android:layout_gravity="center"/>
