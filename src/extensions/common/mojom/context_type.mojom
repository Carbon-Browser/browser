module extensions.mojom;

// The context type associated with a running JS context.
// This should match the values in Feature::Context.
enum ContextType {
  kPrivilegedExtension,
  kUnprivilegedExtension,
  kContentScript,
  kWebPage,
  kPrivilegedWebPage,
  kWebUi,
  kUntrustedWebUi,
  kLockscreenExtension,
  kOffscreenExtension,
  kUserScript,
};
